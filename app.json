{
  "name": "ZohoInvoice",
  "description": "Enjoy ZohoInvoice Integration on YellowAnt",
  "keywords": ["python3", "django2","yellowant"],
  "env": {
    "YA_DEVELOPER_TOKEN": {
      "description": "YellowAnt Developer Token",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku App name",
      "required": true
    },
    "ZOHO_CLIENT_ID": {
      "description": "ZohoInvoice Client ID",
      "required": true
    },
    "ZOHO_CLIENT_SECRET": {
      "description": "ZohoInvoice Client Secret",
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": false,
      "value": "1"
    },
    "ENV": {
      "required": false,
      "value": "HEROKU"
    }

  }
}
